mkdir -p  ../locale/fr/LC_MESSAGES/
mkdir -p ../locale/en_US/LC_MESSAGES/
mkdir -p ../locale/es_ES/LC_MESSAGES/
mkdir -p ../locale/de_DE/LC_MESSAGES/
mkdir -p  ../locale/it/LC_MESSAGES/

msgfmt tweaktool-usermenu-fr.po -o ../locale/fr/LC_MESSAGES/tweaktool-usermenu.mo
msgfmt tweaktool-usermenu-en_US.po -o ../locale/en_US/LC_MESSAGES/tweaktool-usermenu.mo
msgfmt tweaktool-usermenu-es_ES.po -o ../locale/es_ES/LC_MESSAGES/tweaktool-usermenu.mo
msgfmt tweaktool-usermenu-de_DE.po -o ../locale/de_DE/LC_MESSAGES/tweaktool-usermenu.mo
msgfmt tweaktool-usermenu-it.po -o ../locale/it/LC_MESSAGES/tweaktool-usermenu.mo

